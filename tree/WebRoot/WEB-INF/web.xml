<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>tree</display-name>
  <servlet>
    <servlet-name>ClassServlet</servlet-name>
    <servlet-class>com.eduask.servlet.ClassServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StuServlet</servlet-name>
    <servlet-class>com.eduask.servlet.StuServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GraServlet</servlet-name>
    <servlet-class>com.eduask.servlet.GraServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>ClassServlet</servlet-name>
    <url-pattern>/ClassServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StuServlet</servlet-name>
    <url-pattern>/StuServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GraServlet</servlet-name>
    <url-pattern>/GraServlet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>